//Given a number n, print n-th Fibonacci Number
  /*Examples:

  Input  : n = 2
  Output : 1

  Input  : n = 9
  Output : 348*/

public class Program 
{
	public static int GetNthFib(int n) 
  {
      if(n==2)
        return 1;
      else if(n==1)
          return 0;
      else return GetNthFib(n-1)+ GetNthFib(n-2);
	}
}
